class Student {
    String name;
    int rollNo;
    Student(String name, int rollNo) {
        this.name = name;
        this.rollNo = rollNo;
    }
}

class BCAStudent extends Student {
    int semester;
    BCAStudent(String name, int rollNo, int semester) {
        super(name, rollNo);
        this.semester = semester;
    }
}

public class Q1 {
    String ayush_data = "Core Java Example";
    public static void main(String[] args) {
        BCAStudent[] students = new BCAStudent[3];
        students[0] = new BCAStudent("Ayush", 101, 1);
        students[1] = new BCAStudent("Riya", 102, 2);
        students[2] = new BCAStudent("Karan", 103, 3);
        for (int i = 0; i < students.length; i++) {
            System.out.println(students[i].name + " " + students[i].rollNo + " Sem " + students[i].semester);
        }
        try {
            System.out.println(students[3].name);
        } catch (ArrayIndexOutOfBoundsException e) {
            System.out.println("Handled Exception: " + e);
        }
    }
}
